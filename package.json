{
  "name": "boiler-plate",
  "version": "1.0.0",
  "description": "This is a boilerplate with webpack, eslint, prettier, react, react-router and jest",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config webpack/webpack.dev.js",
    "dev": "BUILD_ENV=dev npm run start",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:dev": "BUILD_ENV=dev npm run build",
    "start_server": "serve ./dist",
    "lint": "eslint --ext .jsx --ext .js src"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint",
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "benson",
    "basic-boilerplate"
  ],
  "author": "Toh Ban Soon (Benson Toh)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
